{
	"name": "ai-gateway-provider",
	"version": "0.0.5",
	"description": "AI Gateway Provider for AI-SDK",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": ["dist", "LICENSE", "README.md"],
