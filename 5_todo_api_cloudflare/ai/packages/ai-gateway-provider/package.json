		"url": "git+ssh://git@github.com/cloudflare/ai.git"
	},
	"bugs": {
		"url": "https://github.com/cloudflare/ai/issues"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"husky": "^9.1.6",
		"tsup": "^8.3.5",
		"typescript": "^5.6.3"
	},
	"dependencies": {
		"@ai-sdk/provider": "^1.0.11",
		"@ai-sdk/provider-utils": "^2.1.13",
		"ai": "^4.1.61"
